# Redis Configuration
# ================================
# PENTING: Ganti GANTI_PATH dengan absolute path folder Redis kamu
# Contoh: E:/DATABASE/REDIS/redis_6384

# Network
bind 127.0.0.1
port GANTI_PORT
timeout 0
tcp-keepalive 300

# General
daemonize no
pidfile GANTI_PATH/data/redis_GANTI_PORT.pid
loglevel notice
logfile GANTI_PATH/logs/redis.log

# Data Directory (HARUS absolute path untuk Windows Service)
dir GANTI_PATH/data

# Snapshotting (RDB)
save 900 1
save 300 10
save 60 10000
dbfilename dump_GANTI_PORT.rdb

# Append Only File (AOF)
appendonly yes
appendfilename "appendonly_GANTI_PORT.aof"
appendfsync everysec

# Memory Management
maxmemory 256mb
maxmemory-policy allkeys-lru

# Security
requirepass GANTI_PASSWORD

# Limits
maxclients 100
